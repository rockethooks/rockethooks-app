# RocketHooks Environment Variables
# Copy this file to .env and fill in your values

# GraphQL Configuration
VITE_GRAPHQL_URL=http://localhost:4000/graphql
VITE_WS_URL=ws://localhost:4000/graphql

# Authentication (Clerk)
VITE_CLERK_PUBLISHABLE_KEY=pk_test_your_clerk_publishable_key_here
CLERK_SECRET_KEY=sk_test_your_clerk_secret_key_here
CLERK_JWT_KEY=your_jwt_key_here
VITE_CLERK_SIGN_IN_URL=/sign-in
VITE_CLERK_SIGN_UP_URL=/sign-up
VITE_CLERK_AFTER_SIGN_IN_URL=/dashboard
VITE_CLERK_AFTER_SIGN_UP_URL=/onboarding

# Feature Flags
VITE_ENABLE_ANALYTICS=false
VITE_ENABLE_DEBUG=false

# Public URLs
VITE_PUBLIC_URL=http://localhost:3000

# Demo credentials for development only - DO NOT use in production
VITE_DEMO_EMAIL=adnene@rockethooks.com
VITE_DEMO_PASSWORD=DemoAccess2024!
